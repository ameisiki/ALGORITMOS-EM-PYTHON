v1=[]
v2=[]
v3=[]

def intercala(v1,v2):
    j=5-1
    for i in range(5):
        v3.append(v1[i])
        v3.append(v2[j])
        j=j-1
    return v3


for i in range(5):
    temp=int(input("v1:"))
    v1.append(temp)
for j in range(5):
    temp=int(input("v2:"))
    v2.append(temp)
v3=intercala(v1,v2)
print(v3)


